    "type": "Project",
    "license": "MIT",
    "authors": [
        {
            "name": "Shashwat Mishra",
            "email": "contact@shashwat.cf"
        }
    ],
    "autoload": {
        "psr-4": {
            "Secrethash\\Smsg\\": "src/"
        }
